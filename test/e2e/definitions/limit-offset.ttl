<> a p:Pipeline, p:ReadableObjectMode ;
  p:steps
    [
      p:stepList
        (
          [ base:streamValues ( "{ age: 34 }"^^code:EcmaScript "{ age: 23 }"^^code:EcmaScript "{ age: 10 }"^^code:EcmaScript) ]
